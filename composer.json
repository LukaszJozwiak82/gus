        "gusapi/gusapi": "^5.0",
        "laravelcollective/html": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0@dev"
    },
    "scripts": {
        "test": ".\\vendor\\bin\\phpunit"
    }
