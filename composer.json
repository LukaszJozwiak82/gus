{
    "name": "lukasz/gus",
    "license": "MIT",
    "description": "GUS library for Laravel 6.2",
    "type": "library",
    "authors": [
        {
            "name": "≈Åukasz Jozwiak",
            "email": "admin@admin.pl"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Ljozwiak\\Gus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ljozwiak\\Gus\\Tests\\": "tests"
        }
